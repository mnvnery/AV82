<script>
gsap.registerPlugin(ScrollTrigger);

ScrollTrigger.defaults({
markers: false
});

$('.grid-img-item').eq(0).addClass('is--active');
$('.text-wrapper').eq(0).addClass('is--active');
// Run a function
$(".text-wrapper").each(function (index) {
  let triggerElement = $(this);
  let myIndex = $(this).index();
  let targetElement = $(".grid-img-item").eq(myIndex);
  let tl = gsap.timeline({
    scrollTrigger: {
      trigger: triggerElement,
      // trigger element - viewport
      start: "top center",
      end: "bottom 40%",
      onEnter: () => {
        $(".grid-img-item").removeClass("is--active");
        targetElement.addClass("is--active");
        $(".text-wrapper").removeClass("is--active");
        triggerElement.addClass("is--active");
      },
      onEnterBack: () => {
        $(".grid-img-item").removeClass("is--active");
        targetElement.addClass("is--active");
        $(".text-wrapper").removeClass("is--active");
        triggerElement.addClass("is--active");
      }
    }
  });
});
</script>

